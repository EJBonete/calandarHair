<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EditarCitaActivity">

    <TextView
        android:id="@+id/textView_nuevaCita2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:fontFamily="@font/delisia"
        android:gravity="center_horizontal"
        android:text="Edición Cita"
        android:textColor="#000000"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/vector_person2"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_alignParentStart="true"
        android:drawableStart="@drawable/ic_person" />

    <EditText
        android:id="@+id/editText_name_edicionCitas2"
        android:layout_width="271dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/vector_person"
        android:alpha="0.95"
        android:background="#0000"
        android:ems="10"
        android:hint="Nombre"
        android:inputType="textPersonName"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/vector_contact2"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_alignParentStart="true"
        android:drawableStart="@drawable/ic_contact" />

    <EditText
        android:id="@+id/editText_contact_edicionCitas2"
        android:layout_width="271dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/vector_contact"
        android:alpha="0.95"
        android:background="#0000"
        android:ems="10"
        android:hint="Contacto"
        android:inputType="textPersonName"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/vector_date2"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_alignParentStart="true"
        android:drawableStart="@drawable/ic_date_range" />

    <TextView
        android:id="@+id/hora_edicionCitas2"
        android:layout_width="101dp"
        android:layout_height="26dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:alpha="0.95"
        android:background="#0000"
        android:ems="10"
        android:gravity="clip_horizontal"
        android:text="Hora"
        android:textAlignment="gravity"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/editText_fecha_edicionCitas2"
        android:layout_width="wrap_content"
        android:layout_height="27dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_toStartOf="@+id/hora_edicionCitas"
        android:layout_toEndOf="@+id/vector_date"
        android:alpha="0.95"
        android:background="#0000"
        android:ems="10"
        android:text="Dia"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/vector_service2"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_alignParentStart="true"
        android:drawableStart="@drawable/ic_work" />

    <EditText
        android:id="@+id/editText_service_edicionCitas2"
        android:layout_width="271dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/vector_service"
        android:alpha="0.95"
        android:background="#0000"
        android:ems="10"
        android:hint="Servicio"
        android:inputType="textPersonName"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/vector_others2"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_alignParentStart="true"
        android:drawableStart="@drawable/ic_others" />

    <EditText
        android:id="@+id/editText_others_edicionCitas2"
        android:layout_width="271dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/vector_others"
        android:alpha="0.95"
        android:background="#0000"
        android:ems="10"
        android:hint="Observaciones"
        android:inputType="textPersonName"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/button_3_drop_edicionCitas2"
        android:layout_width="145dp"
        android:layout_height="48dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:backgroundTint="#D7D7D7"
        android:fontFamily="@font/montserrat_alternates_extrabold"
        android:text="GUARDAR"
        android:textAlignment="center"
        android:textColor="#0B0000"
        android:textSize="16sp"
        app:cornerRadius="5dp"
        app:iconTint="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/button_2_save_nuevaCita2"
        android:layout_width="145dp"
        android:layout_height="48dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:backgroundTint="#D7D7D7"
        android:fontFamily="@font/montserrat_alternates_extrabold"
        android:text="BORRAR"
        android:textAlignment="center"
        android:textColor="#DA0E0E"
        android:textSize="16sp"
        app:cornerRadius="5dp"
        app:iconTint="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/button_2_back_nuevaCita"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/button_2_back_nuevaCita2"
        android:layout_width="145dp"
        android:layout_height="48dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:backgroundTint="#D7D7D7"
        android:fontFamily="@font/montserrat_alternates_extrabold"
        android:text="atrás"
        android:textAlignment="center"
        android:textColor="#0B0000"
        android:textSize="16sp"
        app:cornerRadius="5dp"
        app:iconTint="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>